{
  "name": "@worker-render/hono",
  "version": "0.1.0",
  "description": "Universal Hono middleware for server-side rendering with Service Worker support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "hono",
    "ssr",
    "middleware",
    "universal",
    "service-worker"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ashafovaloff/WorkerRender.git",
    "directory": "packages/hono"
  },
  "homepage": "https://github.com/ashafovaloff/WorkerRender#readme",
  "bugs": "https://github.com/ashafovaloff/WorkerRender/issues",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@worker-render/core": "workspace:*"
  },
  "peerDependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "hono": "^4.6.0",
    "typescript": "^5.4.0"
  }
