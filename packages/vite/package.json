{
  "name": "@worker-render/vite",
  "version": "0.1.0",
  "description": "Vite plugin for WorkerRender - handles 3-bundle build (client/sw/worker)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "client"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "vite",
    "plugin",
    "ssr",
    "service-worker",
    "universal"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ashafovaloff/WorkerRender.git",
    "directory": "packages/vite"
  },
  "homepage": "https://github.com/ashafovaloff/WorkerRender#readme",
  "bugs": "https://github.com/ashafovaloff/WorkerRender/issues",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@worker-render/core": "workspace:*"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "typescript": "^5.4.0",
    "vite": "^5.4.0"
  }
}
