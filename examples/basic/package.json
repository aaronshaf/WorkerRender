{
  "name": "worker-render-example-basic",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite build && vite build --config vite.config.sw.ts && tsx server.ts",
    "build": "vite build && npm run build:sw && npm run build:worker",
    "build:sw": "vite build --config vite.config.sw.ts",
    "build:worker": "vite build --config vite.config.worker.ts",
    "build:static": "npm run build && tsx build-static.ts",
    "preview": "npm run build && tsx server.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.0",
    "@worker-render/core": "workspace:*",
    "@worker-render/hono": "workspace:*",
    "hono": "^4.6.0",
    "idiomorph": "^0.7.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.15",
    "@worker-render/vite": "workspace:*",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.15",
    "tsx": "^4.20.6",
    "typescript": "^5.4.0",
    "vite": "^5.4.0"
  }
}
